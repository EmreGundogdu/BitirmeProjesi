@model List<Category>

<div class="list-group">
    <a asp-action="List" asp-controller="Products" class="list-group-item list-group-item-action">Tüm Kategoriler</a>
    @foreach (var category in Model)
    {
        <a asp-action="List" asp-controller="Products" asp-route-id="@category.CategoryId" class="list-group-item list-group-item-action">@category.Name</a>
    }
</div>   